			<header>
				<h4> Your Log List </h4>
			</header>
			<footer>
			<form {{action "login" on="submit"}}>
				<div class="control-group">
					<div class="control-label">
						<label> Details </label>
					</div>
					<div class="control">
						{{input class="input-group" value=details placeholder="Details of your session" type="text"}}
					</div>
				</div>

				<div class="control-group">
					<div class="control-label">
						<label> Log Hours </label>
					</div>
					<div class="control">
						{{input class="input-group" value=duration placeholder="How many hours?" type="text"}}
					</div>
				</div>

				<div class="control-group">
					<div class="control-label">
						<label> Weaknesses </label>
					</div>
					<div class="control">
						{{input class="input-group" value=weakness placeholder="What were weaknesses?" type="text"}}
					</div>
				</div>
				<button type="submit" class="btn btn-primary">Submit</button>
			</form>
